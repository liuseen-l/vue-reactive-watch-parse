#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/f/forTest/monorope2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/f/forTest/monorope2/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../registry.npmmirror.com+c8@7.12.0/node_modules/c8/bin/c8.js" "$@"
else
  exec node  "$basedir/../../../../../../registry.npmmirror.com+c8@7.12.0/node_modules/c8/bin/c8.js" "$@"
fi
