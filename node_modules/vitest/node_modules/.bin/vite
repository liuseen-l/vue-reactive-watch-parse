#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/f/forTest/v-rmake/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/f/forTest/v-rmake/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../registry.npmmirror.com+vite@3.2.3_@types+node@18.11.9/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../registry.npmmirror.com+vite@3.2.3_@types+node@18.11.9/node_modules/vite/bin/vite.js" "$@"
fi
